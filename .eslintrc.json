{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-plusplus": "off",
        "no-console": "off",
        "indent": [
            "error",
            4
        ]
    }
}
